{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source"
      ]
    }
  },
  "plugins": [
    ["module-resolver", {
        "alias": {
          "components": "./src/components",
          "reducers": "./src/reduxFlow/reducers",
          "assets": "./assets",
          "config": "./src/config",
          "images": "./assets/images",
          "services": "./src/services",
          "navigators": "./src/navigators"
        }
      }]
  ]
}
